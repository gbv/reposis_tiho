<mycoreclass xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:noNamespaceSchemaLocation="MCRClassification.xsd" ID="mir_genres">
    <label xml:lang="en" text="genre" description="A list of publication types"/>
    <label xml:lang="x-uri" text="http://www.mycore.org/classifications/mir_genres"/>
    <label xml:lang="de" text="Genre" description="Liste der Publikationsarten"/>
    <categories>
        <category ID="publacad">
            <label xml:lang="de" text="Veröffentlichungen"/>
            <label xml:lang="en" text="Publications"/>
            <label xml:lang="x-group" text="true"/>
            <category ID="article">
                <label xml:lang="x-hosts" text="journal collection festschrift proceedings"/>
                <label xml:lang="x-mapping" text="marcgt:article diniPublType2022:Article schemaOrg:ScholarlyArticle"/>
                <label xml:lang="en" text="Article / Chapter"/>
                <label xml:lang="de" text="Artikel / Kapitel"/>
            </category>
            <category ID="book">
                <label xml:lang="de" text="Buch"/>
                <label xml:lang="x-mapping" text="marcgt:book diniPublType2022:Book schemaOrg:Book"/>
                <label xml:lang="x-hosts" text="series standalone"/>
                <label xml:lang="en" text="Book"/>
            </category>
            <category ID="collection">
                <label xml:lang="x-mapping" text="marcgt:series diniPublType2022:EditedCollection schemaOrg:Book"/>
                <label xml:lang="de" text="Herausgeberwerk"/>
                <label xml:lang="it" text="Compilazione"/>
                <label xml:lang="en" text="Edited Book"/>
                <label xml:lang="x-hosts" text="series standalone"/>
            </category>
            <category ID="proceedings">
                <label xml:lang="x-mapping" text="marcgt:conference_publication diniPublType2022:ConferenceProceedings schemaOrg:Book"/>
                <label xml:lang="en" text="Proceedings"/>
                <label xml:lang="de" text="Tagungsband"/>
                <label xml:lang="x-hosts" text="series standalone"/>
            </category>
            <category ID="poster">
                <label xml:lang="x-mapping" text="marcgt:technical_drawing diniPublType2022:ConferencePoster schemaOrg:PresentationDigitalDocument"/>
                <label xml:lang="en" text="Poster / Abstract"/>
                <label xml:lang="de" text="Poster / Abstract"/>
                <label xml:lang="x-hosts" text="proceedings standalone"/>
            </category>
            <category ID="interview">
                <label xml:lang="x-hosts" text="journal standalone"/>
                <label xml:lang="en" text="Interview"/>
                <label xml:lang="de" text="Interview"/>
                <label xml:lang="x-mapping" text="marcgt:interview diniPublType2022:Report schemaOrg:Report"/>
            </category>
        </category>
        <category ID="thesis">
            <label xml:lang="en" text="Thesis"/>
            <label xml:lang="de" text="Hochschulschriften"/>
            <label xml:lang="x-group" text="true"/>
            <label xml:lang="x-hosts" text="series standalone"/>
            <label xml:lang="x-mapping" text="marcgt:thesis schemaOrg:Thesis XMetaDissPlusThesisLevel:other"/>
            <category ID="dissertation">
                <label xml:lang="de" text="Dissertation"/>
                <label xml:lang="x-mapping" text="marcgt:thesis diniPublType2022:PhDThesis schemaOrg:Thesis XMetaDissPlusThesisLevel:thesis.doctoral"/>
                <label xml:lang="x-hosts" text="series standalone"/>
                <label xml:lang="en" text="Dissertation"/>
            </category>
            <category ID="habilitation">
                <label xml:lang="de" text="Habilitation"/>
                <label xml:lang="x-hosts" text="series standalone"/>
                <label xml:lang="x-mapping" text="marcgt:thesis diniPublType2022:Habilitation schemaOrg:Thesis XMetaDissPlusThesisLevel:thesis.habilitation"/>
                <label xml:lang="en" text="Habilitation"/>
            </category>
            <category ID="diploma_thesis">
                <label xml:lang="x-mapping" text="marcgt:thesis diniPublType2022:MasterThesis schemaOrg:Thesis XMetaDissPlusThesisLevel:Diplom"/>
                <label xml:lang="x-hosts" text="series standalone"/>
                <label xml:lang="en" text="Diploma thesis"/>
                <label xml:lang="de" text="Diplomarbeit"/>
            </category>
            <category ID="master_thesis">
                <label xml:lang="en" text="Master thesis"/>
                <label xml:lang="x-hosts" text="series standalone"/>
                <label xml:lang="x-mapping" text="marcgt:thesis diniPublType2022:MasterThesis schemaOrg:Thesis XMetaDissPlusThesisLevel:master"/>
                <label xml:lang="de" text="Masterarbeit"/>
            </category>
        </category>
        <category ID="publvar">
            <label xml:lang="en" text="Other documents"/>
            <label xml:lang="x-group" text="true"/>
            <label xml:lang="de" text="Andere Dokumente"/>
            <category ID="journal">
                <label xml:lang="x-hosts" text="standalone"/>
                <label xml:lang="de" text="Zeitschrift"/>
                <label xml:lang="en" text="Journal"/>
                <label xml:lang="x-mapping" text="marcgt:journal diniPublType2022:Periodical schemaOrg:Periodical"/>
            </category>
            <category ID="series">
                <label xml:lang="x-hosts" text="standalone"/>
                <label xml:lang="x-mapping" text="marcgt:series diniPublType2022:Other schemaOrg:BookSeries"/>
                <label xml:lang="en" text="Series"/>
                <label xml:lang="de" text="Serie"/>
            </category>
            <category ID="festschrift">
                <label xml:lang="en" text="Festschrift"/>
                <label xml:lang="x-mapping" text="marcgt:festschrift diniPublType2022:Report schemaOrg:Report"/>
                <label xml:lang="x-hosts" text="series standalone"/>
                <label xml:lang="de" text="Festschrift"/>
            </category>
            <category ID="video">
                <label xml:lang="x-hosts" text="standalone"/>
                <label xml:lang="de" text="Film / Video"/>
                <label xml:lang="en" text="Movie / Video"/>
                <label xml:lang="x-mapping" text="marcgt:motion_picture diniPublType2022:MovingImage schemaOrg:MediaObject"/>
            </category>
        </category>
        <category ID="publlib">
            <label xml:lang="de" text="Interne Publikationstypen"/>
            <label xml:lang="en" text="Backoffice publication types"/>
            <label xml:lang="x-group" text="true"/>
            <category ID="research_data">
                <label xml:lang="x-hosts" text="standalone"/>
                <label xml:lang="de" text="Forschungsdaten"/>
                <label xml:lang="en" text="Research Data"/>
                <label xml:lang="x-mapping" text="marcgt:database diniPublType2022:ResearchData schemaOrg:Dataset"/>
            </category>
            <category ID="speech">
                <label xml:lang="en" text="Lecture / Presentation"/>
                <label xml:lang="x-mapping" text="marcgt:article diniPublType2022:Other schemaOrg:Article"/>
                <label xml:lang="de" text="Vortrag / Präsentation"/>
                <label xml:lang="x-hosts" text="proceedings standalone"/>
            </category>
            <category ID="teaching_material">
                <label xml:lang="x-hosts" text="standalone"/>
                <label xml:lang="de" text="Lehrmaterial"/>
                <label xml:lang="x-mapping" text="marcgt:instruction diniPublType2022:CourseMaterial schemaOrg:LearningResource"/>
                <label xml:lang="en" text="Teaching Resource"/>
            </category>
            <category ID="patent">
                <label xml:lang="x-hosts" text="standalone"/>
                <label xml:lang="x-mapping" text="marcgt:patent diniPublType2022:Patent schemaOrg:CreativeWork"/>
                <label xml:lang="de" text="Patent"/>
                <label xml:lang="en" text="Patent"/>
            </category>
        </category>
    </categories>
</mycoreclass>